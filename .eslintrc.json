{
    "env": {
        "node": true,
        "mocha": true
    },
    "extends": "airbnb-base",
    "plugins": [],
    "rules": {
        "new-cap": 0,
        "prefer-template": 0,
        "global-require": 0,
        "no-unused-expressions": 0,
        "no-underscore_dangle": 0,
        "import/no-dynamic-require": 0
    },
    "globals": {
        "describe": true,
        "it": true
    }
}